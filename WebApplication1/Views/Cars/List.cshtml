@model IEnumerable<WebApplication1.Models.Car>

@{
    ViewBag.Title = "List";
    Layout = null;
}

<div id="nowosci">
    <table border=1px>
        @foreach (var car in Model)
        {
            <tr>
                <td><img src="nowkaSztuka.jpg" width=33%></td>
                <td>@car.Mark @car.Model</td>
                <td>@car.Price PLN </td>
                <td>Dodaj do koszyka</td>
            </tr>
        }
    </table>
</div>
